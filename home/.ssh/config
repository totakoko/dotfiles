Host *
  ServerAliveInterval 120

  # reuse existing connections
  ControlMaster auto
  ControlPath ~/.ssh/ctl/%r@%h-%p
  ControlPersist 600

  User root

Host dreau.fr
  Port 2222

Host dev.local
  Port 2222
  IdentityFile ~/.vagrant.d/insecure_private_key

Host 10.10.10.10
  IdentityFile ~/.vagrant.d/insecure_private_key
